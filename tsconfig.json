{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "es2017"
    ],
    "module": "commonjs",
    "declaration": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitUseStrict": true,
    "strictNullChecks": true
  }
}
